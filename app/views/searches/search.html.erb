<div class="col-md-9 skillattributes">
	<table class="table table-bordered table-hover spacing-table">
		<thead>
			<tr>
				<th class="col-md-2">User</th>
				<th class="col-md-5">Service</th>
				<th class="col-md-1">Subcategory</th>
				<th class="col-md-1">Price</th>
				<th class="col-md-1">Zipcode</th>
			</tr>
		</thead>
		<tbody>
			<% @skills.each do |skill| %> 
				<tr>
					<td class="searchrow"><%=skill.user %></td>
					<td class="searchrow"><%=skill.title %></td>
					<td class="searchrow"><%=skill.subcategory_id %></td>
					<td class="searchrow"><%=skill.price %></td>

				</tr>
			<% end %>
		</tbody>
	</table>
</div>


<div class="col-md-4 googlemap">
	<div id="googleMap"></div>
</div>

<%= @skills %>

<%= @subcategories.inspect %>