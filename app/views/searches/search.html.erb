<div class="col-md-8">
	<table class="table table-bordered table-hover spacing-table">
		<thead>
			<tr>
				<th class="col-md-1 table1"><%= sortable "user", "User" %></th>
				<th class="col-md-3 table1"><%= sortable "title", "Service" %></th>
				<th class="col-md-1 table1"><%= sortable "subcategory_id", "Subcategory" %></th>
				<th class="col-md-1 table1"><%= sortable "price", "Price" %></th>
				<th class="col-md-1 table1"><%= sortable "zipcode", "Zipcode" %></th>
			</tr>
		</thead>
		<tbody>
			<% @skills.each do |skill| %> 
				<tr>
					<td class="searchrow"><%=skill.user.fname %><br>
					<%= image_tag skill.user.image %>
					</td>
					<td class="searchrow"><strong><%=skill.title %></strong><br>
					<%= skill.description %>
					</td>
					<td class="searchrow"><%=skill.subcategory.name %></td>
					<td class="searchrow"><%= number_to_currency(skill.price) %></td>
					<td class="searchrow"><%=skill.zipcode %></td>
					<td class="searchrow"><%=link_to('See this Ad', user_skill_path(skill.user, skill.id))%></td>
				</tr>
			<% end %>
		</tbody>
	</table>
	<%= will_paginate @skills%>
</div>

<div class="col-md-4 googlemap">
	<div id="googleMap"></div>
</div>
