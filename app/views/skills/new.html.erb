<%= form_for @skill do |f| %>

<div>
     <%= f.label :title %>
     <%= f.text_field :title %>
</div>

<div>
    <%= f.label :subcategory_id %>
    <%= f.text_field :subcategory_id %>
</div>

<div>
     <%= f.label :price %>
	 <%= f.number_field :price %>
</div>

<div>
    <%= f.label :address %>
    <%= f.text_field :address %>
</div>
<div>
    <%= f.label :city %>
    <%= f.text_field :city %>
</div>
<div>
    <%= f.label :zipcode %>
    <%= f.text_field :zipcode %>
</div>
<div>
    <%= f.label :state %>
    <%= f.text_field :state %>
</div>

 <%= f.submit "Add new skill" %>
<% end %>

<%= link_to "Back to the list!", skills_path %>